    "cross-env": "^7.0.3",
    "docxtemplater": "^3.61.1",
    "dotenv": "^16.5.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.1",
    "pizzip": "^3.1.8",
    "react": "19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-router": "^7.4.1",
    "router": "^2.0.0",
